{
  "name": "skinny-salmon-cookies",
  "version": "1.0.0",
  "scripts": {
    "dev": "routify -c my-server",
    "build": "routify -b && rollup -c",
    "my-server": "rollup -c -w",
    "start": "sirv public --single",
    "lint": "eslint -c .eslintrc --ext .svelte,.js src",
    "format": "eslint -c .eslintrc --fix --ext .svelte,.js --fix src",
    "format:prettier": "prettier --write src/**/{*.svelte,*.js}"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@roxi/routify": "^2.12.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
